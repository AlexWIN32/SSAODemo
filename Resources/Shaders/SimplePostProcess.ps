Texture2D colorTex :register(t0);
SamplerState SampleType :register(s0);

struct PIn
{
    float4 position : SV_POSITION;
    float2 tex : TEXCOORD0;	
};

float4 ProcessPixel(PIn input) : SV_TARGET
{
    return colorTex.Sample(SampleType, input.tex); 
}

